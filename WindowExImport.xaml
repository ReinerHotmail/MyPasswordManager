<Window x:Name="WindowMyExImport" x:Class="MyPasswordManager.WindowExImport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyPasswordManager"
        mc:Ignorable="d"
        Title="WindowExImport" Height="450" Width="280">
    <Grid>
        <StackPanel x:Name="StackPanelUserHlp" Margin="10,10,10,10">
            <Label x:Name="LabelUserNameHlp" Content="User-Name" Margin="0,2" />
            <TextBox x:Name="TextBoxUserHlp" Margin="0,2" FontSize="12"  />
            <Label x:Name="LabelPasswordHlp" Content="Password" Margin="0,2" />
            <PasswordBox x:Name="MyPasswordBoxHlp" Margin="0,2" FontSize="12"  />
            <Label Content=" " Margin="0,2" />
            <Button x:Name="ButtonExport" Content="Export" FontSize="12" Click="ButtonExport_Click" />
            <Button x:Name="ButtonImport" Content="Import" FontSize="12" Click="ButtonImport_Click" />
        </StackPanel>
    </Grid>
</Window>
